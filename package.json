    "axios": "^1.13.4",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "compression": "^1.8.1",
    "cors": "^2.8.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-rate-limit": "^8.2.1",
    "fs": "0.0.1-security",
